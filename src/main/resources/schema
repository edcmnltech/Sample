DROP TABLE `deebee`.`chatroom`;
CREATE TABLE chatroom (
   id INT NOT NULL AUTO_INCREMENT,
   name VARCHAR(50),
   creator VARCHAR(300),
   password VARCHAR(50),
   PRIMARY KEY (id, name));

DROP TABLE `deebee`.`chatmessage`;
CREATE TABLE chatmessage (
  id INT NOT NULL AUTO_INCREMENT,
  roomId INT NOT NULL,
  sender VARCHAR(50),
  message VARCHAR(300),
  timeSent LONG,
  PRIMARY KEY (id));

DROP TABLE `deebee`.`chatuser`;
CREATE TABLE chatuser (
  id INT,
  username VARCHAR(50),
  PRIMARY KEY (username));

